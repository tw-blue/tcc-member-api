<!DOCTYPE html>
<html>
  <head>
    <title>Users List</title>
  </head>
  <body>
    <table>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Mitglieder Abrufen?</th>
            <th>Zugriff Verwalten?</th>
        </tr>
        <% for(var i=0; i<users.length; i++) {%>
            <tr>
              <td>
                <%= users[i].name%>
              </td>
              <td>
                <%= users[i].email%>
              </td>
              <td>
                <% if(users[i].viewMembers){ %>
                    <h1>&#x2713;</h1>
                  <% } else{ %>  
                    <h1>&#x2717;</h1>
                 <% } %>
              </td>
              <td>
                <% if(users[i].manageUsers){ %>
                    <h1>&#x2713;</h1>
                  <% } else{ %>  
                    <h1>&#x2717;</h1>
                 <% } %>
              </td>
            </tr>
         <% } %>
    </table>
    </div>
  </body>
</html>